{
  "name": "@importcsv/react",
  "version": "0.3.0",
  "description": "AI-powered CSV importer for React. Smart column mapping with OpenAI, natural language transformations. Self-hosted.",
  "main": "build/react/index.js",
  "module": "build/react/index.esm.js",
  "exports": {
    ".": {
      "types": "./build/react/index.d.ts",
      "import": "./build/react/index.esm.js",
      "require": "./build/react/index.js"
    },
    "./react": {
      "types": "./build/react/index.d.ts",
      "import": "./build/react/index.esm.js",
      "require": "./build/react/index.js"
    },
    "./preact": {
      "types": "./build/preact/types/index.d.ts",
      "import": "./build/preact/index.esm.js",
      "require": "./build/preact/index.js"
    },
    "./bundled": {
      "types": "./build/bundled/types/index.d.ts",
      "script": "./build/bundled/index.umd.js",
      "import": "./build/bundled/index.esm.js",
      "require": "./build/bundled/index.js"
    },
    "./styles.css": "./build/style.css"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/importcsv/importcsv.git"
  },
  "keywords": [
    "csv",
    "import",
    "excel",
    "data",
    "importer",
    "react",
    "csv-import"
  ],
  "author": "ImportCSV",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/importcsv/importcsv/issues"
  },
  "homepage": "https://github.com/importcsv/importcsv#readme",
  "types": "build/react/index.d.ts",
  "scripts": {
    "build:react": "vite build --mode react && cp types/index.d.ts build/react/index.d.ts",
    "build:preact": "vite build --mode preact",
    "build:bundled": "vite build --mode bundled",
    "build": "npm-run-all --parallel build:react build:preact build:bundled",
    "build:legacy": "vite build --mode production",
    "dev": "vite --mode preact",
    "dev:examples": "cd examples && npm install && npm run dev",
    "watch:react": "vite build --watch --mode react",
    "watch:preact": "vite build --watch --mode preact",
    "watch:bundled": "vite build --watch --mode bundled",
    "publish:react": "npm version patch && npm run build:react && npm publish",
    "publish:react:minor": "npm version minor && npm run build:react && npm publish",
    "publish:react:major": "npm version major && npm run build:react && npm publish",
    "yalc:publish": "npm run build:react && yalc publish --push",
    "yalc:watch": "npm run watch:react & yalc publish --push --changed",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:visual": "playwright test",
    "test:all": "npm run test && npm run test:visual",
    "test:local": "npm run build:react && npm link && echo 'Package linked! Run \"npm link @importcsv/react\" in your test app'",
    "test:app": "cd /Users/abhishekray/Projects/importcsv/test/csv-importer-test && npm link @importcsv/react && npm run dev",
    "clean:stale": "find src -name '*.js' -not -path '*/shims/*' -type f -delete && find src -name '*.js.map' -type f -delete && echo 'Cleaned stale JS files'",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "preact": "^10.0.0",
    "react": "^16.8.0 || ^17 || ^18 || ^19",
    "react-dom": "^16.8.0 || ^17 || ^18 || ^19",
    "zod": "^3.20.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "preact": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@preact/compat": "^18.3.1",
    "@preact/preset-vite": "^2.10.2",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/babel__core": "^7.20.1",
    "@types/jest": "^24.9.1",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/use-sync-external-store": "0.0.3",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "core-js": "^3.22.7",
    "css-loader": "^6.8.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "happy-dom": "^19.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jsdom": "^27.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.1",
    "postcss-prefix-selector": "^2.1.1",
    "prettier": "^2.8.6",
    "prop-types": "^15.8.1",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^26.5.6",
    "tsd": "^0.33.0",
    "typescript": "^4.5.5",
    "typescript-plugin-css-modules": "^3.4.0",
    "use-sync-external-store": "^1.2.0",
    "vite": "^7.1.3",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0",
    "zod": "^3.25.76"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-virtual": "^3.13.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.507.0",
    "papaparse": "^5.4.1",
    "react-hotkeys-hook": "^5.1.0",
    "tailwind-merge": "^3.2.0",
    "zustand": "^4.4.7"
  },
  "type": "module"
}
