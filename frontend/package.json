{
  "name": "@importcsv/react",
  "version": "0.2.1",
  "description": "Open-source CSV and XLS/XLSX file importer for React and JavaScript",
  "main": "build/preact/index.js",
  "module": "build/preact/index.esm.js",
  "exports": {
    ".": {
      "types": "./build/preact/types/index.d.ts",
      "import": "./build/preact/index.esm.js",
      "require": "./build/preact/index.js"
    },
    "./react": {
      "types": "./build/react/types/index.d.ts",
      "import": "./build/react/index.esm.js",
      "require": "./build/react/index.js"
    },
    "./preact": {
      "types": "./build/preact/types/index.d.ts",
      "import": "./build/preact/index.esm.js",
      "require": "./build/preact/index.js"
    },
    "./bundled": {
      "types": "./build/bundled/types/index.d.ts",
      "import": "./build/bundled/index.esm.js",
      "require": "./build/bundled/index.js",
      "script": "./build/bundled/index.umd.js"
    }
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhishekray07/importcsv.git"
  },
  "keywords": [
    "csv",
    "import",
    "excel",
    "data",
    "importer",
    "react",
    "csv-import"
  ],
  "author": "ImportCSV",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abhishekray07/importcsv/issues"
  },
  "homepage": "https://github.com/abhishekray07/importcsv#readme",
  "types": "build/index.d.ts",
  "scripts": {
    "build:react": "vite build --mode react",
    "build:preact": "vite build --mode preact",
    "build:bundled": "vite build --mode bundled",
    "build": "npm-run-all --parallel build:react build:preact build:bundled",
    "build:legacy": "vite build --mode production",
    "dev": "vite --mode preact",
    "dev:examples": "cd examples && npm install && npm run dev",
    "watch:react": "vite build --watch --mode react",
    "watch:preact": "vite build --watch --mode preact",
    "watch:bundled": "vite build --watch --mode bundled"
  },
  "peerDependencies": {
    "preact": "^10.0.0",
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "preact": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@preact/compat": "^18.3.1",
    "@preact/preset-vite": "^2.10.2",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/babel__core": "^7.20.1",
    "@types/jest": "^24.9.1",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/use-sync-external-store": "0.0.3",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^5.0.1",
    "core-js": "^3.22.7",
    "css-loader": "^6.8.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^16.1.1",
    "prettier": "^2.8.6",
    "prop-types": "^15.8.1",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.5.5",
    "typescript-plugin-css-modules": "^3.4.0",
    "use-sync-external-store": "^1.2.0",
    "vite": "^7.1.3",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.13.12",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.507.0",
    "papaparse": "^5.4.1",
    "postcss": "^8.5.3",
    "preact": "^10.0.0",
    "react-hotkeys-hook": "^5.1.0",
    "tailwind-merge": "^3.2.0",
    "zustand": "^4.4.7"
  },
  "type": "module"
}
